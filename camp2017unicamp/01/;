#include <bits/stdc++.h>
#define ff first
#define ss second 
#define pb push_back
using namespace std;
typedef long long ll;
typedef pair<int, int> pii;

int main () {
    int n = 1000;
    int m = 20000;
    int cont = 0;
    printf("%d %d\n", n, m);
    for(int i = 0; i < n; i++)
        for(int j = i+1; j < n; j++) {
            printf("%d %d\n", i+1, j+1);
            cont++;
            if(cont == m) return 0;
        }
}

